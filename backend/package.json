{
  "name": "ride-my-way",
  "version": "1.0.0",
  "description": "API for ride my way carpooling",
  "scripts": {
    "start": "nodemon --exec babel-node app.js",
    "dev": "NODE_ENV=development npm run watch babel-node app",
    "heroku-prebuild": "rm -rf node_modules",
    "prod": "npm run build && NODE_ENV=production babel-node app",
    "build": "npm run clean && babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "engines": {
    "node": "12.6.0",
    "npm": "6.9.0"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "node_modules/*"
    ],
    "delay": "3500"
  },
  "author": "Bennett Benedict",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/register": "^7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.1.0",
    "bcrypt": "3.0.8",
    "bigint-hash": "^0.2.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "1.4.5",
    "core-js": "3.6.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "8.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.3",
    "express": "^4.17.1",
    "express-session": "1.17.1",
    "http-errors": "1.8.0",
    "jest": "24.9.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.18.4",
    "make-runnable": "1.3.8",
    "morgan": "1.10.0",
    "nodemon": "1.19.4",
    "passport": "0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "7.18.2",
    "pg-hstore": "^2.3.3",
    "pg-promise": "8.7.5",
    "regenerator-runtime": "0.13.7",
    "rimraf": "2.7.1",
    "swagger-editor": "3.14.2",
    "swagger-ui": "3.35.1",
    "swagger-ui-express": "4.1.4",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/node": "7.10.5"
  }
}
